<div class="title-wrap">
<% @titles.each do |title| %>
  <div class="title-box">
    <%= attachment_image_tag list, :image, :fill, 200, 200, format: 'jpeg' %>
    <div class="title-comment">
      <span class="title-body"><%= title.body %></span>
      <span class="title-name"><%= link_to title.name, new_post_path(title.id) %></span>
    </div>
  </div>
<% end %>
</div>